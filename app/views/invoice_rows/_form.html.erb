<%= form_for url: invoice_rows_path, remote: true, format: :js do |row_form| %>
  <%= row_form.label :item %>
  <%= row_form.text_field :item %>
  <%= row_form.label :quantity %>
  <%= row_form.text_field :quantity %>
  <%= row_form.label :unit %>
  <%= row_form.text_field :unit %>
  <%= row_form.label :unit_price %>
  <%= row_form.text_field :unit_price %>
  <%= row_form.label :vat, 'VAT' %>
  <%= row_form.text_field :vat %>
  <%= row_form.submit 'Save row'%>
<% end %>
